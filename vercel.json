{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "headers": [
        {
            "source": "/team/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                },
                {
                    "key": "x-vercel-optimization",
                    "value": "off"
                }
            ]
        }
    ],
    "images": {
        "remotePatterns": [],
        "unoptimized": true
    }
}